      <trans-unit id="tl_faq_category.allowComments.0">
        <source>Enable comments</source>
        <target>Povolit komentáře</target>
      </trans-unit>
      <trans-unit id="tl_faq_category.allowComments.1">
        <source>Allow visitors to comment FAQs.</source>
        <target>Dovolí návštěvníkům přidávat komentáře k FAQ.</target>
      </trans-unit>
      <trans-unit id="tl_faq_category.notify.0">
        <source>Notify</source>
        <target>Zaslat upozornění</target>
      </trans-unit>
      <trans-unit id="tl_faq_category.notify.1">
        <source>Please choose who to notify when comments are added.</source>
        <target>Zvolte prosím osobu, která dostane vyrozumění mailem, že byl přidán komentář.</target>
      </trans-unit>
      <trans-unit id="tl_faq_category.sortOrder.0">
